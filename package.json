{
    "name": "@letrungdo/web-ui",
    "description": "React components for Web UI.",
    "version": "0.1.7",
    "keywords": [
        "React components lib",
        "React Sotrybook",
        "React Rollup Typescript"
    ],
    "author": "Le Trung Do",
    "license": "UNLICENSED",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/letrungdo/react-ui-component-lib.git"
    },
    "scripts": {
        "build": "rm -rf dist && rollup -c",
        "postbuild": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
        "build-watch": "rollup -c -w",
        "sb": "start-storybook -p 9009",
        "build-sb": "build-storybook",
        "lint": "eslint '**/**/*.{ts,tsx}' --quiet --fix",
        "format": "prettier --write '**/*.{scss,json}'",
        "test": "jest",
        "testw": "jest --watch",
        "testc": "jest --collect-coverage"
    },
    "devDependencies": {
        "@emotion/core": "^10.0.28",
        "@emotion/jest": "^11.2.1",
        "@emotion/styled": "^10.0.27",
