        "style-loader": "^2.0.0",
        "ts-jest": "^26.5.4",
        "typescript": "4.2.3"
    },
    "peerDependencies": {
        "@emotion/core": "10.x",
        "@emotion/styled": "10.x",
        "react": "^16.x",
        "react-dom": "^16.x"
    }
